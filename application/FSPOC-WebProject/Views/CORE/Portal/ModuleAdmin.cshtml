            <div class="moduleSquare" moduleName="persona" style="background-image: url(/Content/Images/ModuleIcons/persona.png)"></div>
            <div class="moduleSquare" moduleName="sentry" style="background-image: url(/Content/Images/ModuleIcons/sentry.png)"></div>
            <div class="moduleSquare" moduleName="nexus" style="background-image: url(/Content/Images/ModuleIcons/nexus.png)"></div>
            <div class="moduleSquare selectedSquare" moduleName="athena" style="background-image: url(/Content/Images/ModuleIcons/athena.png)"></div>
            <div class="moduleSquare" moduleName="watchtower" style="background-image: url(/Content/Images/ModuleIcons/watchtower.png)"></div>
