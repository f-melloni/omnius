<div id="moduleConfigPanel">
    <div class="panelLabel">Konfigurace modulu</div>
    <div class="currentModuleIcon" style="background-image: url(/Content/Images/ModuleIcons/athena.png)"></div>
    <div class="currentModuleName">Athena</div>
    <div class="currentModuleDescription">Analytický modul</div>
    <div class="currentModuleLastChange"><b>Poslední změna:</b> 17.4.2015</div>

    <table class="moduleConfigTable">
        <tr class="headerRow">
            <td colspan="2">Hlavní</td>
        </tr>
        <tr>
            <td>Stav</td>
            <td class="inactive">Neaktivní<i class="fa fa-caret-down"></i></td>
        </tr>
        <tr>
            <td>Účet</td>
            <td>athena_service_account</td>
        </tr>
        <tr class="headerRow">
            <td colspan="2">Služby</td>
        </tr>
        <tr>
            <td colspan="2">/service/athena/chart_renderer.xml</td>
        </tr>
        <tr>
            <td colspan="2">/service/athena/analytic_unit.xml</td>
        </tr>
    </table>
</div>