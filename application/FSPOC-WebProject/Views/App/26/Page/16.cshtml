@{ Layout = "~/Views/Shared/_OmniusUserAppLayout.cshtml"; }<form class="mozaicForm" method="post"><div id="uic_label1" name="label1"  class="uic form-heading" contentTemplate="" style="left: 100px; top: 120px; width: 144px; height: 40px; ">Přehled hromadných objednávek</div><div id="uic_breadcrumb2" name="breadcrumb2" class="uic breadcrumb-navigation breadcrumb-navigation" style="left: 90px; top: 45px; width: 1100px; height: 55px; "><div class="app-icon fa @ViewData["appIcon"]"></div><div class="nav-text">@ViewData["appName"] &gt; @ViewData["pageName"]</div></div><button id="uic_export_button" name="button" value="export_button"  class="uic button-simple button-small" buttonName="export_button" tabindex="" style="left: 100px; top: 180px; width: 112px; height: 23px;  ">Export Excel</button>@{ if(ViewData.ContainsKey("tableData_data-table-read-only3") && ((System.Data.DataTable)(ViewData["tableData_data-table-read-only3"])).Rows.Count > 0) {<table id="uic_data-table-read-only3" name="data-table-read-only3"  class="uic data-table display" style="left: 100px; top: 220px; width: 998px; height: 151px; " uicWidth="998px"><thead><tr>@foreach (System.Data.DataColumn col in ((System.Data.DataTable)(ViewData["tableData_data-table-read-only3"])).Columns){<th>@col.Caption</th>}</tr></thead><tfoot><tr>@foreach (System.Data.DataColumn col in ((System.Data.DataTable)(ViewData["tableData_data-table-read-only3"])).Columns){<th>@col.Caption</th>}</tr></tfoot><tbody>@foreach(System.Data.DataRow row in ((System.Data.DataTable)(ViewData["tableData_data-table-read-only3"])).Rows){<tr>@foreach (var cell in row.ItemArray){<td>@cell.ToString()</td>}</tr>}</tbody>} }</table>} else { <div class="uic control-label empty-table-label" style="left: 100px; top: 220px;">Tabulka neobsahuje žádná data</div> } }<input type="hidden" name="data-table-read-only3" /></form>