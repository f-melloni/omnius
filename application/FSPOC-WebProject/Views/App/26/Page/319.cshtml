@{ Layout = "~/Views/Shared/_OmniusUserAppLayout.cshtml"; }<form class="mozaicForm" method="post"><div id="uic_label1" name="label1"  class="uic form-heading" contentTemplate="" style="left: 100px; top: 120px; width: 144px; height: 40px; ">Dynamický filtr</div><div id="uic_breadcrumb2" name="breadcrumb2" class="uic breadcrumb-navigation breadcrumb-navigation" style="left: 90px; top: 45px; width: 1100px; height: 55px; "><div class="app-icon fa @ViewData["appIcon"]"></div><div class="nav-text">@ViewData["appName"] &gt; @ViewData["pageName"]</div></div><button id="uic_button_export" name="button" value="button_export"  class="uic button-simple button-small" buttonName="button_export" tabindex="" style="left: 100px; top: 200px; width: 150px; height: 23px;  ">Exportovat Excel</button>@{ if(ViewData.ContainsKey("tableData_data-table") && ((System.Data.DataTable)(ViewData["tableData_data-table"])).Rows.Count > 0) {<table id="uic_data-table" name="data-table"  class="uic data-table display" style="left: 100px; top: 240px; width: 988px; height: 151px; " uicWidth="988px"><thead><tr>@foreach (System.Data.DataColumn col in ((System.Data.DataTable)(ViewData["tableData_data-table"])).Columns){<th>@col.Caption</th>}</tr></thead><tfoot><tr>@foreach (System.Data.DataColumn col in ((System.Data.DataTable)(ViewData["tableData_data-table"])).Columns){<th>@col.Caption</th>}</tr></tfoot><tbody>@foreach(System.Data.DataRow row in ((System.Data.DataTable)(ViewData["tableData_data-table"])).Rows){<tr>@foreach (var cell in row.ItemArray){<td>@cell.ToString()</td>}</tr>}</tbody>} }</table>} else { <div class="uic control-label empty-table-label" style="left: 100px; top: 240px;">Tabulka neobsahuje žádná data</div> } }<input type="hidden" name="data-table" /></form>