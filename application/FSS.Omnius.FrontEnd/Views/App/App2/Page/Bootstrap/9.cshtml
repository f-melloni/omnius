@{ Layout = "~/Views/Shared/_OmniusUserAppLayout.cshtml"; }@{ Dictionary<string, string> formState = (Dictionary<string, string>)ViewData["formState"]; }<div class="mozaicBootstrapPage"><div class="container"><p class="text-danger bg-danger"><label class="" id="Messages">@(@"{var1}".Replace("{var1}", (ViewData.FirstOrDefault(v => v.Key.EndsWith("Messages")).Value ?? "").ToString()))</label></p><div class="panel panel-default" ><div class="panel-heading"><h3 class="panel-title">@(@"Persons")</h3></div><div class="panel-body">
@{ if(ViewData.ContainsKey("tableData_DataTables_Table_0") && ((System.Data.DataTable)(ViewData["tableData_DataTables_Table_0"])).Rows.Count > 0) {
    <table class="table data-table no-footer dataTable table-striped" data-dtpaging="0" data-dtinfo="0" data-dtfilter="0" data-dtordering="1" data-dtcolumnfilter="0" data-dtserverside="0" id="DataTables_Table_0" role="grid" aria-describedby="DataTables_Table_0_info">
        <thead>
            <tr>
            @foreach (System.Data.DataColumn col in ((System.Data.DataTable)(ViewData["tableData_DataTables_Table_0"])).Columns) {
                <th>@col.Caption</th>
            }
                
            </tr>
        </thead>
        <tfoot>
            <tr>@foreach (System.Data.DataColumn col in ((System.Data.DataTable)(ViewData["tableData_DataTables_Table_0"])).Columns) {
                <th>@col.Caption</th>
            }
                
            </tr>
        </tfoot>
        <tbody>
        @foreach(System.Data.DataRow row in ((System.Data.DataTable)(ViewData["tableData_DataTables_Table_0"])).Rows) {
            <tr>@foreach (var cell in row.ItemArray) {
                <td>@cell.ToString()</td>
            }
                
            </tr>
        }
        </tbody>
    </table>
} else {
    <div class="alert alert-info empty-table-label">Tabulka neobsahuje žádná data</div>
} }
<input type="hidden" name="DataTables_Table_0" />
<p class="" id="label_totalWeight">@Html.Raw(@"<br><b><span class=""mbe-text-node"" contenteditable=""false"">Total weight: {var1}</span></b><br>".Replace("{var1}", (ViewData.FirstOrDefault(v => v.Key.EndsWith("label_totalWeight")).Value ?? "").ToString()))</p><p class="" id="label_totalAge">@Html.Raw(@"<b><span class=""mbe-text-node"">Total age: {var1}</span></b>".Replace("{var1}", (ViewData.FirstOrDefault(v => v.Key.EndsWith("label_totalAge")).Value ?? "").ToString()))</p><p class="" id="label_living">@Html.Raw(@"<b>Count living: {var1}</b>".Replace("{var1}", (ViewData.FirstOrDefault(v => v.Key.EndsWith("label_living")).Value ?? "").ToString()))</p></div></div><div class="panel panel-default" ><div class="panel-heading"><h3 class="panel-title">@(@"Import CSV test")</h3></div><div class="panel-body"><form class="form-horizontal" method="post" enctype="multipart/form-data"><div class="form-group"><label for="file_csv" class="col-sm-2">@(@"Soubor:")</label><div class="col-xs-12 col-sm-10"><input type="file" name="file_csv" value="@(formState.ContainsKey("file_csv") ? formState["file_csv"] : (ViewData.ContainsKey("inputData_file_csv") ? @ViewData["inputData_file_csv"] : ""))" class="form-control" id="file_csv" /></div></div><button type="submit" class="btn btn-primary" name="button" value="btn_submit" id="btn_submit" >@(@"Submit")</button>@Html.AntiForgeryToken()</form></div></div></div></div>