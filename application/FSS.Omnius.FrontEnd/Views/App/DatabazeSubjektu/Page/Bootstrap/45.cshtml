@{ Layout = "~/Views/Shared/_OmniusUserAppLayout.cshtml"; }@{ Dictionary<string, string> formState = (Dictionary<string, string>)ViewData["formState"]; }<div class="mozaicBootstrapPage"><div class="container-fluid"><div class="row"><form class="form-horizontal" method="post"><div class="col-xs-12"><h1 class="">@(@"Přehled subjektů")</h1>
@if((ViewData.ContainsKey("yes_panel") && (bool)ViewData["yes_panel"]) == true) {
<div class="" data-varname="yes_panel"><div class="panel panel-default" ><div class="panel-body"><p class="">@(@"V databázi nalezeno více subjektů se stejným IČO.")</p><p class="">@(@"„Povinný subjekt“ dle zákona o registru smluv:")</p><p class=""><span class="" style="color: red;">@(@"ANO ")</span></p><p class=""><span class="">@(@"Pokud je smlouva s hodnotou plnění vyšší než 50 000 Kč bez DPH, nutno uveřejnit v registru smluv!")</span></p></div></div></div>
}
@if((ViewData.ContainsKey("no_panel") && (bool)ViewData["no_panel"]) == true) {
<div class="" data-varname="no_panel"><div class="panel panel-default" ><div class="panel-body"><p class="mbe-active">@(@"V databázi nalezeno více subjektů se stejným IČO.")</p><p class="">@(@"„Povinný subjekt“ dle zákona o registru smluv:")</p><p class="" style="color:red;">@(@"NE")</p></div></div></div>
}
@{ if(ViewData.ContainsKey("tableData_DataTables_Subjekt") && ((System.Data.DataTable)(ViewData["tableData_DataTables_Subjekt"])).Rows.Count > 0) {
    <table class="table data-table no-footer dataTable" data-dtpaging="1" data-dtinfo="1" data-dtfilter="1" data-dtordering="1" data-dtcolumnfilter="1" id="DataTables_Subjekt" role="grid" aria-describedby="DataTables_Subjekt_info" data-dtserverside="0" style="">
        <thead>
            <tr>
            @foreach (System.Data.DataColumn col in ((System.Data.DataTable)(ViewData["tableData_DataTables_Subjekt"])).Columns) {
                <th>@col.Caption</th>
            }
                <th>Akce</th>
            </tr>
        </thead>
        <tfoot>
            <tr>@foreach (System.Data.DataColumn col in ((System.Data.DataTable)(ViewData["tableData_DataTables_Subjekt"])).Columns) {
                <th>@col.Caption</th>
            }
                <th>Akce</th>
            </tr>
        </tfoot>
        <tbody>
        @foreach(System.Data.DataRow row in ((System.Data.DataTable)(ViewData["tableData_DataTables_Subjekt"])).Rows) {
            <tr>@foreach (var cell in row.ItemArray) {
                <td>@cell.ToString()</td>
            }
                <td class="actionIcons"><i title="detail" class="fa fa-sign-in" data-action="detail" data-idparam="modelId" data-confirm=""></i></td>
            </tr>
        }
        </tbody>
    </table>
} else {
    <div class="alert alert-info empty-table-label">Tabulka neobsahuje žádná data</div>
} }
<input type="hidden" name="DataTables_Subjekt" />
</div>@Html.AntiForgeryToken()</form></div><div class=""><style>
  #DataTables_Subjekt_wrapper .fa-pencil {margin-right: 10px;}
</style></div></div></div>