@{ Layout = "~/Views/Shared/_OmniusUserAppLayout.cshtml"; }@{ Dictionary<string, string> formState = (Dictionary<string, string>)ViewData["formState"]; }<div class="mozaicBootstrapPage"><div class="container"><div class="panel panel-default" ><div class="panel-heading"><h3 class="panel-title">@(@"Send message to RabbitMQ")</h3></div><div class="panel-body"><div class="col-xs-12"><form class="form-horizontal" method="post"><div class="form-group"><div class="form-group"><label for="" class="col-sm-2">@(@"Message:")</label><div class="col-xs-12 col-sm-10"><input type="text" name="message" value="@(formState.ContainsKey("message") ? formState["message"] : (ViewData.ContainsKey("inputData_message") ? @ViewData["inputData_message"] : ""))" class="form-control" id="message" /></div></div></div><div class="form-group"><button type="submit" class="btn btn-primary" name="button" value="Send" id="Send" ><span class="fa fa-chevron-right"></span>&nbsp; Send</button></div>@Html.AntiForgeryToken()</form></div></div></div></div></div>