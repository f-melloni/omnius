@{ Layout = "~/Views/Shared/_OmniusUserAppLayout.cshtml"; }@{ Dictionary<string, string> formState = (Dictionary<string, string>)ViewData["formState"]; }<div class="mozaicBootstrapPage"><form class="form-horizontal" method="post"><div class="container"><div class="panel panel-default" ><div class="panel-heading"><h3 class="panel-title">@(@"TM Fee user")</h3></div><div class="panel-body">
