@using FSS.Omnius.FrontEnd.Utils@{T t = new T( (string)ViewData["locale"] );}@{ Layout = "~/Views/Shared/_OmniusUserAppLayout.cshtml"; }<form class="mozaicForm" method="post" enctype = "multipart/form-data"><div id="uic_panel1" name="panel1"  class="uic panel-component visible-panel-pale-background"  style="left: 20px; top: 120px; width: 1200px; height: 620px;  ">@{ if(ViewData.ContainsKey("tableData_prehled_table") && ((System.Data.DataTable)(ViewData["tableData_prehled_table"])).Rows.Count > 0) {<table id="uic_prehled_table" name="prehled_table"  class="uic data-table data-table-with-actions display" style="left: 20px; top: 40px; width: 1000px; height: 151px; " uicWidth="1000px"><thead><tr>@foreach (System.Data.DataColumn col in ((System.Data.DataTable)(ViewData["tableData_prehled_table"])).Columns){<th>@(t._(col.Caption))</th>}<th>@(t._("Akce"))</th></tr></thead><tfoot><tr>@foreach (System.Data.DataColumn col in ((System.Data.DataTable)(ViewData["tableData_prehled_table"])).Columns){<th>@(t._(col.Caption))</th>}<th>@(t._("Akce"))</th></tr></tfoot><tbody>@foreach(System.Data.DataRow row in ((System.Data.DataTable)(ViewData["tableData_prehled_table"])).Rows){<tr>@foreach (var cell in row.ItemArray){<td>@cell.ToString()</td>}<td class="actionIcons"><i class="fa fa-edit rowEditAction"></i><i class="fa fa-remove rowDeleteAction"></i></td></tr>}</tbody></table>} else { <div class="uic control-label empty-table-label" style="left: 20px; top: 40px;">Tabulka neobsahuje žádná data</div> } }<input type="hidden" name="prehled_table" /></div><div id="uic_Přehled" name="Přehled"  class="uic form-heading" contentTemplate="" style="left: 20px; top: 40px; width: 146px; height: 40px; ">@Html.Raw(ViewData.ContainsKey("inputData_Přehled") ? "Form heading".Replace("{var1}", ViewData["inputData_Přehled"].ToString()) : "Form heading" )</div><input type="hidden" name="registry" value="@ViewData["crossBlockRegistry"]" />@Html.AntiForgeryToken()</form>